{
  "name": "st-common-ui-vue3",
  "version": "0.14.0",
  "description": "小尾巴通用 Vue3 UI 组件库",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "lib",
    "es",
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:cssall": "vue-tsc --noEmit && vite build --config vite.config.css-all.ts",
    "build:csssplit": "vue-tsc --noEmit && vite build --config vite.config.css-split.ts",
    "build": "pnpm build:csssplit && pnpm build:cssall"
  },
  "keywords": [
    "小尾巴",
    "组件库",
    "small tail",
    "common",
    "ui",
    "vue",
    "vue3",
    "lib"
  ],
  "author": "ST",
  "license": "ISC",
  "dependencies": {
    "@vueuse/core": "^12.0.0",
    "st-common-core": "^0.1.1",
    "st-common-req": "0.2.2",
    "st-common-ui-utils": "^0.1.1",
    "swiper": "^11.1.15",
    "unplugin-vue-components": "^0.27.5",
    "vanilla-tilt": "^1.8.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@iconify-icons/tabler": "^1.2.95",
    "@iconify/types": "^2.0.0",
    "@iconify/vue": "^4.1.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "sass-embedded": "^1.81.0",
    "typescript": "5.6.2",
    "vue-tsc": "^2.1.10"
  }
}
